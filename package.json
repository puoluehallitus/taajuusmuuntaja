{
  "name": "Taajuusmuuntaja",
  "version": "0.0.1",
  "description": "Puoluehallitus - Taajuusmuuntaja",
  "author": "Janne Sysi-Aho <janne.sysiaho@gmail.com>",
  "license": "MIT",
  "scripts": {
    "prestart": "npm run clean",
    "start": "cross-env NODE_ENV=development parcel serve ./src/index.html",
    "preelectron": "npm run clean",
    "electron": "npm-run-all -p build:watch launch",
    "build": "npm run build:prod",
    "build:prod": "cross-env NODE_ENV=production parcel build --public-url '.' --no-content-hash ./src/index.html",
    "build:dev": "cross-env NODE_ENV=development parcel build --public-url '.' --no-content-hash ./src/index.html",
    "build:watch": "cross-env NODE_ENV=development parcel watch --public-url '.' --no-content-hash ./src/index.html",
    "clean": "rimraf ./dist ./puoluehallitus-taajuusmuuntaja-win32-x64 ./puoluehallitus-taajuusmuuntaja-win32-x64.zip",
    "prelaunch": "wait-on ./dist/index.html",
    "launch": "cross-env NODE_ENV=development electron .",
    "prepackage": "npm-run-all -s clean build",
    "package": "npx electron-packager . puoluehallitus-taajuusmuuntaja --ignore=\"(.gitignore|.node-version|.parcel-cache|node_modules|src)\" --no-prune --platform=win32 --arch=x64",
    "postpackage": "cross-zip ./puoluehallitus-taajuusmuuntaja-win32-x64/ ./puoluehallitus-taajuusmuuntaja-win32-x64.zip"
  },
  "devDependencies": {
    "@electron/packager": "^18.1.3",
    "@tsconfig/recommended": "1.0.3",
    "@types/stats": "0.16.30",
    "@types/three": "0.161.2",
    "cross-env": "7.0.3",
    "cross-zip-cli": "1.0.0",
    "electron": "29.0.0",
    "electron-packager": "17.1.2",
    "electron-reload": "2.0.0-alpha.1",
    "fs-extra": "11.2.0",
    "npm-run-all": "4.1.5",
    "parcel": "2.11.0",
    "rimraf": "5.0.5",
    "stats.js": "0.17.0",
    "three": "0.161.0",
    "typescript": "5.3.3",
    "wait-on": "7.2.0"
  }
}
